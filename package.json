{
  "name": "react-redux-instagram",
  "version": "0.0.1",
  "description": "use react x redux build instagram.",
  "scripts": {
    "start": "node index.js",
    "lint": "eslint src",
    "test": "NODE_ENV=test ava -v",
    "test:cov": "nyc npm test"
  },
  "ava": {
    "require": "babel-core/register",
    "files": "test/**/*.spec.js",
    "babel": "inherit"
  },
  "nyc": {
    "all": true,
    "sourceMap": false,
    "instrument": false,
    "require": "babel-core/register",
    "exclude": "test/**/*.spec.js"
  },
  "bin": {
    "eslint": "node_modules/.bin/eslint"
  },
  "keywords": [
    "react",
    "redux",
    "universal",
    "instagram"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neighborhood999/react-redux-instagram.git"
  },
  "author": {
    "name": "Peng Jie",
    "url": "https://github.com/neighborhood999"
  },
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-istanbul": "^2.0.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "cookie-parser": "^1.4.3",
    "css-loader": "^0.23.1",
    "eslint": "^3.3.0",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-import": "^1.13.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.1.0",
    "express": "^4.14.0",
    "fetch-jsonp": "^1.0.1",
    "file-loader": "^0.9.0",
    "isomorphic-fetch": "^2.2.1",
    "less-loader": "^2.2.3",
    "nock": "^8.0.0",
    "nyc": "^8.1.0",
    "redux-devtools": "^3.3.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "redux-mock-store": "^1.1.2",
    "request-promise": "^4.1.1",
    "style-loader": "^0.13.1",
    "unix-timestamp-transform": "0.0.5",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "font-awesome": "^4.6.3",
    "jquery": "^2.2.3",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0"
  }
